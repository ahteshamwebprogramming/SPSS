
@{
    ViewBag.Title = "QualityReport";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div class="all_reports">
    <div class="content-wrapper qualityReport">
        <div class="container-fluid">
            <div class="row mt-4">
                <div class="col-md-8">
                    <div class="card-body">
                        <h2>Quality Report</h2>
                        <div class="attr_icons_a mt-3">
