                                        <div class="inner_selector">
                                            <ul>
                                                <li>
                                                    <label class="containerChk">
                                                        Delhi/NCR
                                                        <input type="checkbox" checked="checked">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>
                                                <li>
                                                    <label class="containerChk">
                                                        Banglore
                                                        <input type="checkbox">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>
                                                <li>
                                                    <label class="containerChk">
                                                        Mumbai
                                                        <input type="checkbox">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>
                                                <li>
                                                    <label class="containerChk">
                                                        Delhi/NCR
                                                        <input type="checkbox">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>
                                                <li>
                                                    <label class="containerChk">
                                                        Banglore
                                                        <input type="checkbox">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>
                                                <li>
                                                    <label class="containerChk">
                                                        Mumbai
                                                        <input type="checkbox">
                                                        <span class="checkmark"></span>
                                                    </label>
                                                </li>

                                            </ul>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-3">
                                    <select name="" id="">
                                        <option value="">Operators</option>
                                        <option value="">Greter Then</option>
                                        <option value="">Equal to</option>
                                        <option value="">Less Then</option>
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <input type="number" placeholder="Percentage">

                                </div>

                            </div>
                        </div>
                        <div class="p_block mt-3">
                            <div class="row">
                                <div class="col-md-12 pb-2">
                                    <h3>Last interviewed</h3>
                                </div>
                            </div>
                            <div class="row">
                                <div class="col-md-3">
                                    <select name="" id="">
                                        <option value="">Operators</option>
                                        <option value="">Demo2</option>
                                        <option value="">Demo3</option>
                                    </select>
                                </div>
                                <div class="col-md-3">
                                    <input type="number" placeholder="Values">
                                </div>
                                <div class="col-md-3">
                                    <select name="" id="">
                                        <option value="">Metric</option>
                                        <option value="">Day</option>
                                        <option value="">Month</option>
                                        <option value="">Year</option>
                                    </select>
                                </div>
                            </div>
                        </div>
                    </div>
                    <a href="#" class="save uni_btn">Submit</a>
                </div>
                <div class="col-md-4">
                    <div class="card-body chart_area">
                        <h2>Summary Report</h2>
                        <div class="table-responsive mt-3">
                            <table class="table">
                                <thead class="thead-dark">
                                    <tr>
                                        <th scope="col">Quality Parameter</th>
                                        <th scope="col">Total N</th>
                                        <th scope="col">Error N</th>
                                        <th scope="col">Error%</th>
                                    </tr>
                                </thead>
                                <tbody>
