                                <div class="project_name">
                                    <select name="" id="">
                                        <option value="">Demo1</option>
                                        <option value="">Demo2</option>
                                        <option value="">Demo3</option>
                                    </select>
                                </div>
                                <div class="toggler">
                                    <span class="mr-2">
                                        Length of Interview <label class="switch">
                                            <input type="checkbox">
                                            <span class="slider round"></span>
                                        </label>
                                    </span>
                                    <span class="mr-2">
                                        Straight linner <label class="switch">
                                            <input type="checkbox" checked>
                                            <span class="slider round"></span>
                                        </label>
                                    </span>
                                    <span>
                                        Last Interview
